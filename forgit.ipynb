   "source": [
    "infilename = input(\"입력 파일 이름 : \")\n",
    "infile = open(infilename, \"r\")\n",
    "rfile = infile.read()\n",
    "alp = {}\n",
    "\n",
    "for line in rfile:\n",
    "    for ch in line:\n",
    "        if ch in alp:\n",
    "            alp[ch] += 1\n",
    "        else:\n",
    "            alp[ch] = 1\n",
    "print(alp)\n",
    "\n",
    "infile.close()\n"
   ]
  }
